{% if collection.length > 0 %}
  <section class="section">
    <h2 class="section__title">{{ title }}</h2>
    <ul class="section__list">
    {% for item in collection reversed %}
      <li class="section__list-item">
        {%- include "item" %}
      </li>
    {% endfor %}
    </ul>
  </section>
{% endif %}
