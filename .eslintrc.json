{
    "root": true,
    "env": {
        "es6": false
    },
    "globals": {
        "google": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "no-new": [
            "off"
        ],
        "import/no-extraneous-dependencies": [
            "error", {
                "devDependencies": true
            }
        ]
    }
}
